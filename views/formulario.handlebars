<br>
<br>
<table class="table">
    <tr>
        <th>Nome</th>
        <th>Senha</th>
        <th>Atualizar</th>
        <th>Excluir</th>
    </tr>
    {{#each doador}}
        <tr>
            <td>{{nome}}</td>
            <td>{{senha}}</td>
            <td><a href="#">Modificar</a></td>  
            <td><a href="/delete/{{id}}">Apagar</a></td>
        </tr>
    {{/each}}
</table>